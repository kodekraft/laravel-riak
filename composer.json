{
    "name": "pds/laravel-riak",
    "description": "A Riak based Eloquent model and Query builder for Laravel (Riakuent)",
    "type": "project",
    "require": {
        "basho/riak": "^3.1",
        "illuminate/support": "^5.2",
        "illuminate/container": "^5.2",
        "illuminate/database": "^5.2",
        "illuminate/events": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "mockery/mockery": "^0.9.5",
        "orchestra/testbench": "^3.1",
        "ramsey/uuid": "^3.4"
    },
    "autoload": {
        "psr-0": {
            "Riakuent\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/models"
        ]
    }
}
